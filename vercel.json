{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "functions": {
    "api/twiml.ts": {
      "maxDuration": 10
    },
    "api/twilio/conversation/webhook.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/twiml",
      "destination": "/api/twiml"
    }
  ]
}

